
<div class="title-page">
<h2>Ironhack Cinema</h2>
<h3>Click on the movie to check the showtimes!</h3>
</div>
<div class="cards-container">
  {{#each movies}}
    <div class="card my-1" style="width: 18rem;">
      <img src="{{image}}" alt="">
      <h5 class="cards-title">{{title}}</h5>
      <div class="link-movie">
      <a href="/movies/{{_id}}">See more</a>
      </div>
    </div>
    
  {{/each}}
</div>
